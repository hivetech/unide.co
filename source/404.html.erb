---
layout: base
---

<!-- header-10 -->
<header class="header-10">
  <div class="container">
    <div class="navbar span12">
      <div class="navbar-inner">
        <button type="button" class="btn btn-navbar"> </button>
        <a class="brand" href="#"><img src="images/<%= data.design.project_logo %>" width="100" height="100" alt=""></a>

        <div class="nav-collapse collapse pull-right">
          <ul class="nav">
            <li class="active"><a href="mailto:<%= data.project.contact %>">Feedback</a></li>
            <li><a href="https://github.com/<%= data.project.github %>">Github</a></li>
            <li><a href="<%= data.project.blog %>">Blog</a></li>
          </ul>
          <form class="navbar-form pull-left">
            <a class="btn bg-clouds" href="/#sign-up"><%= data.project.action_button %></a>
          </form>
        </div>
      </div>
    </div>
  </div>
</header>

<!-- content-8  -->
<section class="content-8 v-center">
  <div class="container">
    <h3>404 - Hyper Apocalyptic Fatal Error</h3>
    <div class="row">
      <div class="span6 offset3">
        <p>
          Sorry, the page you tried to visit doesn't exist.
        </p>
        <a class="btn btn-large btn-clear" href="/">Come back</a>
      </div>
    </div>
  </div>
</section>
